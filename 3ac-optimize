t1 = 2 + 3		| t1 = 5							|
t0 = 1 + t1		| t0 = 1 + t1(5) ∴ t0 = 6	| 
i = t0			| i = t0 ∴ i = 6				| i = 6
t2 = i - 4		| t2 = i - 4 ∴ t2 = 2		|
i = t2			| i = t2 ∴ i = 2				| i = 2
t4 = i - 20		| t4 = i - 20 ∴ t4 = -18	|
t3 = 3 + t4		| t3 = 3 + t4 ∴ t3 = -15	|
i = t3			| i = t3 ∴ i = -15			| i = -15
i = -1			| i = -1							| i = -1
